{
  "insightsConfig": {
    "name": "Loglines count",
    "database": "monitoring",
    "schema": "main"
  },
  "sqlTemplates": [
    {
      "sql": "SELECT SUM(lines) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(CASE WHEN logLevel = 'error' THEN lines ELSE 0 END) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(CASE WHEN logLevel = 'warn' THEN lines ELSE 0 END) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(CASE WHEN logLevel = 'info' THEN lines ELSE 0 END) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(CASE WHEN logLevel = 'debug' THEN lines ELSE 0 END) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(CASE WHEN logLevel IS NULL OR logLevel != 'debug' AND logLevel != 'info' AND logLevel != 'warn' AND logLevel != 'error' THEN lines ELSE 0 END) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".lines WHERE {{{globalWhereFilter}}} AND hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    },
    {
      "sql": "SELECT SUM(lines) AS value, hour FROM \"{{{database}}}\".\"{{{schema}}}\".errorlines WHERE hour BETWEEN '{{{startHour}}}' AND '{{{endHour}}}' GROUP BY hour ORDER BY hour;"
    }
  ],
